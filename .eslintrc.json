{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "camelcase": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "no-restricted-syntax": "off",
    "max-classes-per-file": "off"
  }
}
